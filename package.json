{
  "name": "dslama.net",
  "version": "3.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules && npm i",
    "codegen": "rm -rf src/graphql/generated && graphql-codegen --config codegen.yml",
    "codegen:watch": "npm run codegen -- --watch",
    "predev": "prisma generate",
    "dev": "concurrently -k -p \"{time}-{pid}\" npm:codegen:watch npm:typecheck:watch \"next dev\"",
    "clean:dev": "npm run clean && npm run dev",
    "dev:debug": "concurrently -k -p \"{time}-{pid}\" npm:codegen:watch \"NODE_OPTIONS='--inspect' next dev\"",
    "build": "npm run codegen && prisma generate && next build",
    "start": "next start -p 8080",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx src",
    "typecheck": "tsc --noEmit --pretty",
    "typecheck:watch": "npm run typecheck -- --watch --preserveWatchOutput",
    "image:build": "sudo docker build -f ../deployment/dslama.net/Dockerfile -t deanslamajr/dslama.net:\"$(git describe)\" .",
    "image:promote": "sudo docker push deanslamajr/dslama.net:\"$(git describe)\"",
    "publish": "npm run image:build && npm run image:promote",
    "publish:test": "npm version prerelease --preid=g\"$(git rev-parse --short=5 HEAD)\" && npm run publish",
    "publish:patch": "npm version patch && npm run publish",
    "publish:minor": "npm version minor && npm run publish",
    "publish:major": "npm version major && npm run publish"
  },
  "dependencies": {
    "@apollo/client": "^3.4.10",
    "@prisma/client": "^3.0.2",
    "apollo-server-micro": "^2.10.1",
    "aws-sdk": "^2.643.0",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "cookie-session": "^1.4.0",
    "dotenv": "^8.2.0",
    "final-form": "^4.20.2",
    "final-form-arrays": "^3.0.2",
    "final-form-focus": "^1.1.2",
    "graphql": "^15.5.2",
    "graphql-tag": "^2.10.3",
    "grommet": "^2.17.5",
    "grommet-icons": "^4.6.2",
    "next": "^9.3.4",
    "next-connect": "^0.10.2",
    "polished": "^4.1.3",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-final-form": "^6.5.3",
    "react-final-form-arrays": "^3.1.3",
    "styled-components": "^5.3.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.1.1",
    "@graphql-codegen/fragment-matcher": "3.1.0",
    "@graphql-codegen/introspection": "2.1.0",
    "@graphql-codegen/typescript": "2.2.0",
    "@graphql-codegen/typescript-document-nodes": "2.1.3",
    "@graphql-codegen/typescript-graphql-files-modules": "2.1.0",
    "@graphql-codegen/typescript-operations": "2.1.3",
    "@graphql-codegen/typescript-react-apollo": "3.1.3",
    "@graphql-codegen/typescript-resolvers": "^2.2.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-session": "^2.0.43",
    "@types/node": "^13.7.7",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "concurrently": "^6.2.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "prettier": "^1.19.1",
    "prisma": "^3.0.2",
    "typescript": "^4.5.2"
  }
}
