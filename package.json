{
  "name": "dslama.net",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules && npm i",
    "codegen": "graphql-let",
    "dev": "npm run codegen && next dev",
    "dev:debug": "npm run codegen && NODE_OPTIONS='--inspect' next dev",
    "build": "npm run codegen && next build",
    "start": "next start -p 8080",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx src",
    "image:build": "sudo docker build -f ../deployment/dslama.net/nextjs-refactor/Dockerfile -t deanslamajr/dslama.net:\"$(git describe)\" .",
    "image:promote": "sudo docker push deanslamajr/dslama.net:\"$(git describe)\"",
    "publish": "npm run image:build && npm run image:promote",
    "publish:test": "npm version prerelease --preid=g\"$(git rev-parse --short=5 HEAD)\" && npm run publish",
    "publish:patch": "npm version patch && npm run publish",
    "publish:minor": "npm version minor && npm run publish",
    "publish:major": "npm version major && npm run publish"
  },
  "dependencies": {
    "@apollo/react-common": "^3.1.3",
    "@apollo/react-components": "^3.1.3",
    "@apollo/react-hooks": "^3.1.3",
    "@apollo/react-ssr": "^3.1.3",
    "@awspilot/dynamodb": "^2.0.0",
    "@prisma/client": "^2.29.1",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-http": "^1.5.16",
    "apollo-link-schema": "^1.2.4",
    "apollo-server-micro": "^2.10.1",
    "apollo-utilities": "^1.3.3",
    "aws-sdk": "^2.643.0",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "next": "^9.3.4",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.12.2",
    "@graphql-codegen/plugin-helpers": "^1.12.2",
    "@graphql-codegen/typescript": "^1.12.2",
    "@graphql-codegen/typescript-operations": "^1.12.2",
    "@graphql-codegen/typescript-react-apollo": "^1.12.2",
    "@graphql-codegen/typescript-resolvers": "^1.12.2",
    "@types/node": "^13.7.7",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "graphql-let": "^0.9.3",
    "prettier": "^1.19.1",
    "prisma": "^2.29.1",
    "typescript": "^3.8.3"
  }
}
